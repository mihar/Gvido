%tr.clickable
  %td
    = link_to student.full_name, student
  %td
    = contact_links(student.personal_contacts)
  %td
    = program_mentor(student.active_enrollments)
  %td
    = full_address(student)
  %td
    = student.landline
  %td
    = student.mobile
  %td
    = student.email
  %td
    = student.status.short_description
  %td
    = l student.created_at, :format => :short

